# Begin msmtprc 
# Set default values for all following accounts.
defaults
tls_trust_file ~/trust.pem
tls on
logfile ~/.msmtp.log

# A first gmail address
account wa.milton@gmail.com
host smtp.gmail.com
port 587
protocol smtp
auth on
from wa.milton@gmail.com
user wa.milton@gmail.com
tls_starttls on

# A second gmail address
account will@promethost.com
host smtp.gmail.com
port 587
protocol smtp
auth on
from will@promethost.com
user will@promethost.com
tls_starttls on
